{
  "name": "typescript-starter",
  "version": "1.2.0",
  "description": "field_description",
  "main": "index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "npm run cover",
    "check-coverage": "nyc check-coverage --lines 65 --functions 60 --Statements 65",
    "ava": "NODE_ENV=test ava",
    "server": "node server",
    "lint": "tslint --fix --project tsconfig.json -t stylish",
    "qualitycheck": "npm-run-all typecheck tslint",
    "cover": "tsc && NODE_ENV=test npm run lint && nyc npm run ava",
    "size": "ls -lh dist/",
    "bp": "npm run build && npm run size",
    "build": "rm -rf ./dist && babel src/ --source-maps inline --extensions \".ts,.tsx\" --ignore **/__test__/**,src/**/**.test.ts -d dist",
    "build:watch": "babel --watch src/**/*.ts -d dist",
    "changelog-major": "build-changelog --major",
    "changelog-minor": "build-changelog --minor",
    "changelog-patch": "build-changelog --patch"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.test.ts",
      "src/**/__tests__/**"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary",
      "cobertura"
    ],
    "extension": [
      ".tsx",
      ".ts"
    ],
    "cache": true,
    "temp-directory": "./coverage/.nyc_output"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/puncsky/template_website.git"
  },
  "keywords": [],
  "author": "field_author",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/puncsky/template_website/issues"
  },
  "homepage": "https://github.com/puncsky/template_website#readme",
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "7.2.0",
    "@babel/polyfill": "7.2.5",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "7.0.0",
    "@types/node": "^11.10.4",
    "ava": "1.0.1",
    "babel-loader": "8.0.5",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-react-require": "3.1.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "build-changelog": "2.1.2",
    "global": "4.3.2",
    "nyc": "13.3.0",
    "ts-node": "^8.0.2",
    "tslint": "5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-microsoft-contrib": "6.1.0",
    "tslint-require-connnect-typing": "1.0.1",
    "typescript": "^3.3.3333"
  },
  "engines": {
    "node": "10.15.0",
    "npm": "6.9.0"
  },
  "private": true,
  "dependencies": {}
}
